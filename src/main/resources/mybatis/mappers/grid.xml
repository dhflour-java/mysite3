<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="grid">
	
	<select id="getList" resultType="gridvo">
		<![CDATA[
		   select no,
		          name,
			      age,
			      country,
			      address,
			      married 
	         from client
	     order by no desc
		]]>
	</select>
	
</mapper>